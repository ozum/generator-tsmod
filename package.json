{
  "name": "generator-tsmod",
  "version": "0.0.0",
  "description": "Yeoman generator for TypeScript modules with opinionated workflow.",
  "author": {
    "email": "ozum@ozum.net",
    "name": "Özüm Eldoğan",
    "url": "https://github.com/ozum"
  },
  "keywords": [
    "yeoman-generator",
    "typescript"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "main": "generators/index.js",
  "types": "generators/index.d.ts",
  "files": [
    "generators",
    "!generators/**/@(*.spec|*.test)*",
    "!generators/**/__test__",
    "module-files",
    "@types"
  ],
  "homepage": "https://github.com/ozum/generator-tsmod",
  "bugs": "https://github.com/ozum/generator-tsmod/issues",
  "repository": "github:ozum/generator-tsmod",
  "license": "MIT",
  "scripts": {
    "execute": "ts-node --transpile-only --compiler-options '{ \"module\": \"commonjs\" }'",
    "watch": "ts-node-dev --no-notify --respawn --transpile-only --compiler-options '{ \"module\": \"commonjs\" }'",
    "lint": "eslint --ignore-path .gitignore --cache --max-warnings 0 --ext js,jsx,ts,tsx,vue --fix .",
    "format": "prettier --write",
    "release": "npm run readme && git pull && git add -A && git commit && git push --follow-tags",
    "test": "jest --coverage",
    "readme": "if grep -q '{% include \"api.md\" %}' 'README.njk'; then npm run typedoc:single-md; mkdir -p temp && mv api.md temp/; fi && readmeasy --partial-dirs temp,module-files/template-partials && rm -rf temp",
    "build": "tsc --incremental",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "prebuild": "del-cli generators/ && copyup --all --exclude \"**/*.ts\" \"src/**/*\" generators",
    "prepublishOnly": "pinst --disable",
    "update-packages": "find src -name \"package.json\" -exec ncu -u --packageFile {} \\;",
    "yo:update": "mkdir -p module-files/configs && cp src/typescript/templates/tsconfig.base.json module-files/configs/tsconfig.json && npm run build && yo tsmod:uninstall --no-install --force && yo tsmod --no-boilerplate --no-coverage --projectRoot generators"
  },
  "dependencies": {
    "@swc/core": "^1.2.40",
    "array-map-to-object": "0.0.4",
    "comment-json": "^4.1.0",
    "deep-cleaner": "^1.2.1",
    "delete-empty": "^3.0.0",
    "fast-glob": "^3.2.4",
    "generator-license": "^5.4.0",
    "git-remote-origin-url": "^3.1.0",
    "inquirer-npm-name": "^3.0.0",
    "js-yaml": "^3.14.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.difference": "^4.5.0",
    "lodash.differencewith": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.merge": "^4.6.2",
    "lodash.pick": "^4.4.0",
    "lodash.set": "^4.3.2",
    "lodash.uniq": "^4.5.0",
    "lodash.unset": "^4.5.2",
    "lodash.upperfirst": "^4.3.1",
    "object-hash": "^2.0.3",
    "parse-author": "^2.0.0",
    "read-git-user": "^2.1.0",
    "user-meta": "^1.0.0",
    "validate-npm-package-name": "^3.0.0",
    "yeoman-generator": "^4.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@schemastore/package": "0.0.6",
    "@types/delete-empty": "^3.0.0",
    "@types/inquirer-npm-name": "^3.0.1",
    "@types/jest": "^26.0.19",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.difference": "^4.5.6",
    "@types/lodash.differencewith": "^4.5.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isempty": "^4.4.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/lodash.unset": "^4.5.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/node": "^14.14.13",
    "@types/object-hash": "^1.3.4",
    "@types/parse-author": "^2.0.0",
    "@types/validate-npm-package-name": "^3.0.2",
    "@types/yeoman-generator": "^4.11.3",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "commitizen": "^4.1.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^5.0.6",
    "is-ci": "^2.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "npm-check-updates": "^10.2.5",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "readmeasy": "^0.2.7",
    "ts-jest": "^26.2.0",
    "ts-node-dev": "^1.1.1",
    "tslib": "^2.0.1",
    "typescript": "^4.1.3"
  }
}
