{
  "name": "generator-tsmod",
  "version": "0.0.0",
  "author": {
    "name": "Özüm Eldoğan",
    "email": "ozum@ozum.net",
    "url": "https://www.ozum.net/"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "generators/index.js",
  "types": "generators/index.d.ts",
  "files": [
    "generators",
    "!generators/**/@(*.spec|*.test)*",
    "!generators/**/__test__",
    "module-files",
    "config"
  ],
  "homepage": "https://github.com/ozum/generator-tsmod",
  "bugs": "https://github.com/ozum/generator-tsmod/issues",
  "repository": "ozum/generator-tsmod",
  "license": "MIT",
  "scripts": {
    "execute": "ts-node --transpile-only --compiler-options '{ \"module\": \"commonjs\" }'",
    "watch": "ts-node-dev --no-notify --respawn --transpile-only --compiler-options '{ \"module\": \"commonjs\" }'",
    "lint": "eslint --ignore-path .gitignore --cache --max-warnings 0 --ext js,jsx,ts,tsx,vue --fix .",
    "format": "prettier . --write",
    "release": "npm run readme && git pull && git add -A && git commit && git push --follow-tags",
    "test": "jest --coverage",
    "readme": "node module-files/scripts/tsmod.js readme",
    "prebuild": "rm -rf generators && copyfiles --up 1 --all --exclude \"**/*.ts\" \"src/**/*\" generators",
    "build": "tsc",
    "postinstall": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "tsmod": "node module-files/scripts/tsmod.js",
    "yo:update": "yo 'tsmod' '--no-typedoc' '--projectRoot' 'generators'",
    "preyo:update": "npm run build",
    "update-packages": "find src -name \"package.json\" -exec ncu -u --packageFile {} \\;"
  },
  "dependencies": {
    "array-map-to-object": "^0.0.4",
    "fast-glob": "^3.2.7",
    "generator-license": "^5.5.0",
    "git-remote-origin-url": "^3.1.0",
    "inquirer-npm-name": "^4.0.0",
    "js-yaml": "^4.1.0",
    "lodash.assignwith": "^4.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.differencewith": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.merge": "^4.6.2",
    "lodash.pick": "^4.4.0",
    "lodash.set": "^4.3.2",
    "lodash.uniq": "^4.5.0",
    "parse-author": "^2.0.0",
    "read-git-user": "^2.1.0",
    "tslib": "^2.3.1",
    "user-meta": "^1.0.0",
    "validate-npm-package-name": "^3.0.0",
    "yeoman-generator": "^5.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@schemastore/package": "^0.0.6",
    "@types/git-remote-origin-url": "^3.0.1",
    "@types/inquirer-npm-name": "^3.0.2",
    "@types/jest": "^27.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.assignwith": "^4.2.6",
    "@types/lodash.difference": "^4.5.6",
    "@types/lodash.differencewith": "^4.5.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/node": "^16.11.12",
    "@types/parse-author": "^2.0.1",
    "@types/validate-npm-package-name": "^3.0.3",
    "@types/yeoman-generator": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.4.1",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.4.3",
    "json-schema": "^0.4.0",
    "lint-staged": "^12.1.2",
    "npm-check-updates": "^12.0.3",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "readmeasy": "^0.2.7",
    "ts-jest": "^27.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2",
    "walkdir": "^0.4.1"
  }
}
